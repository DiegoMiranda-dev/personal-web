
<section class="px-10 lg:px-72 flex justify-between items-center  mb-8">
<button id="regresar" onclick="regresar()"
  class="text-white bg-gray-800 hover:bg-gray-900 focus:outline-none focus:ring-4 focus:ring-gray-300 font-medium rounded-lg text-sm px-5 py-2.5 me-2 mb-2 dark:bg-gray-800 dark:hover:bg-gray-700 dark:focus:ring-gray-700 dark:border-gray-700"
  >Volver</button
>
<script>

window.onload = function() {
    function regresar() {
        window.history.back()
    }

    const botonRegresar = document.getElementById('regresar');
    if (botonRegresar) {
        botonRegresar.onclick = regresar;
    } else {
        console.log("No se encontró el botón 'regresar'");
    }
}



</script>
<button id="siguiente"
  class="text-gray-900 hover:text-white border border-gray-800 hover:bg-gray-900 focus:ring-4 focus:outline-none focus:ring-gray-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center me-2 mb-2 dark:border-gray-600 dark:text-gray-400 dark:hover:text-white dark:hover:bg-gray-600 dark:focus:ring-gray-800"
  >Siguiente</button>
</section>
